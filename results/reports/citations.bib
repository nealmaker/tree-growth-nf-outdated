@article{arseneault_first_2011,
	title = {First decadal response to treatment in a disturbance-based silviculture experiment in {Maine}},
	volume = {262},
	issn = {03781127},
	url = {https://linkinghub.elsevier.com/retrieve/pii/S0378112711002210},
	doi = {10.1016/j.foreco.2011.04.006},
	abstract = {Disturbance-based silvicultural systems generally seek to promote complex stand structures that are consistent with temporal and spatial patterns of natural disturbance while allowing for the sustainable harvest of timber. Gap-based harvesting systems are commonly used within this framework because they can be designed to approximate the frequencies and spatial patterns of a wide array of disturbance regimes. Patterns in stand-level growth, sapling recruitment and regeneration response were examined for one such gap-based system, the Acadian Forest Ecosystem Research Program (AFERP) in central Maine, that was designed to emulate the annual 1\% disturbance frequency typical of the northeastern United States and Canada. A decade after treatment, stand-level differences in basal area growth and density between two gap-based treatments and an unharvested control were not statistically signiﬁcant, largely due to low replication, but within-stand growth and regeneration responses differed strongly by spatial position relative to harvest gaps. Regeneration of shade-tolerant and intolerant species increased regardless of gap size, likely a response to increasing light availability from canopy openings due to harvesting and mortality. Further, there was evidence of gap size effects on sapling recruitment as large gaps ({\textgreater}1000 m2) favored the growth and survival of mid-successional species such as red maple and white pine, while small gaps ({\textless}1000 m2) favored late-succession species such as eastern hemlock and spruce. Overstory growth rates also differed by both species and position relative to harvest gaps with most species growing best in gaps and better along gap edges than in adjacent forest. Notably, overstory growth rates for white pine were not inﬂuenced by spatial position. These results suggest harvest gaps may have signiﬁcant growth and regeneration impacts in adjacent, yet unharvested areas, which could lead to profound differences in forest development over the rotation. Obviously, longer-term studies of gap-based systems are needed to more clearly elucidate these responses.},
	language = {en},
	number = {3},
	urldate = {2020-01-13},
	journal = {Forest Ecology and Management},
	author = {Arseneault, Justin E. and Saunders, Mike R. and Seymour, Robert S. and Wagner, Robert G.},
	month = aug,
	year = {2011},
	pages = {404--412},
	file = {Arseneault et al. - 2011 - First decadal response to treatment in a disturban.pdf:C\:\\Users\\Neal\\Zotero\\storage\\D4X9VIFJ\\Arseneault et al. - 2011 - First decadal response to treatment in a disturban.pdf:application/pdf}
