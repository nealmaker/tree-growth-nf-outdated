	abstract = {A new taper model was developed based on the switching taper model of Valentine and Gregoire; the most substantial changes were reformulation to incorporate estimated join points and modification of a switching function. Random-effects parameters were included that account for within-tree correlations and allow for customized calibration to each individual tree. The new model was applied to 19 species groups from data collected on 2,448 trees across the northeastern United States. A comparison of fit statistics showed considerable improvement for the new model. The largest absolute residuals occurred near the tree base, whereas the residuals standardized in relation to tree diameter were generally constant along the length of the stem. There was substantial variability in the rate of taper at the base of the tree, particularly for trees with relatively large dbh. Similar trends were found in independent validation data. The use of random effects allows uncertainty in computed tree volumes to compare favorably with another regional taper equation that uses upper-stem diameter information for local calibration. However, comparisons with results for a locally developed model showed that the local model provided more precise estimates of volume. FOR. SCI. 56(6):515–528.},
	language = {en},
	author = {Westfall, James A},
	pages = {14},
	file = {Westfall - Taper Models for Commercial Tree Species in the No.pdf:C\:\\Users\\Neal\\Zotero\\storage\\XHXJ4W7Z\\Westfall - Taper Models for Commercial Tree Species in the No.pdf:application/pdf}
}

@article{cao_predicting_nodate,
	title = {{PREDICTING} {DIAMETER} {AT} {BREAST} {HEIGHT} {FROM} {TOTAL} {HEIGHT} {AND} {CROWN} {LENGTH}},
	abstract = {Tree diameter at breast height (d.b.h.) is often predicted from total height (model 1a) or both total height and number of trees per acre (model 1b). These approaches are useful when Light Detection and Ranging (LiDAR) data are available. LiDAR height data can be employed to predict tree d.b.h., and consequently individual tree volumes and volume/ ha can be obtained for the tract. In this paper, we will examine alternative methods of predicting d.b.h. from total height and crown length (model 2a), or from total height, crown length, and number of trees per acre (model 2b), based on the uniform stress theory. The uniform stress theory hypothesizes that stems behave like tapered cantilever beams to equalize bending stress across their length. The four models were evaluated based on the mean difference between observed and predicted diameters, mean absolute difference, and fit index. Results revealed that the two models based on the uniform stress theory (models 2a and 2b) were more appropriate for predicting d.b.h., which is needed to compute tract volume using LiDAR data.},
	language = {en},
	author = {Cao, Quang V and Dean, Thomas J},
	pages = {5},
	file = {Cao and Dean - PREDICTING DIAMETER AT BREAST HEIGHT FROM TOTAL HE.pdf:C\:\\Users\\Neal\\Zotero\\storage\\SGTD4QF9\\Cao and Dean - PREDICTING DIAMETER AT BREAST HEIGHT FROM TOTAL HE.pdf:application/pdf}
}

