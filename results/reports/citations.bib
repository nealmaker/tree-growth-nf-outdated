@article{engelman_interference_2006,
	title = {Interference to {Hardwood} {Regeneration} in {Northeastern} {North} {America}: {Assessing} and {Countering} {Ferns} in {Northern} {Hardwood} {Forests}},
	volume = {23},
	issn = {0742-6348, 1938-3762},
